{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "script",
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules" : {
    "no-unused-vars": ["error", { "vars": "local", "args": "after-used", "ignoreRestSiblings": false }]
  }
}
